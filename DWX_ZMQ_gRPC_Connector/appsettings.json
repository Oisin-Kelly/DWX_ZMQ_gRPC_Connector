{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DWX_ZMQ_gRPC_Connector.Clients": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "ZmqConfig": {
    "Host": "localhost",
    "Protocol": "tcp",
    "PushPort": 32768,
    "PullPort": 32769,
    "SubPort": 32770,
    "UseMock": false
  }
}
